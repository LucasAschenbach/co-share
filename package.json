{
  "name": "co-share",
  "version": "3.0.3",
  "main": "dist",
  "description": "Architecting shared applications using js & node.js",
  "dependencies": {
    "react": "^17.0.1",
    "rxjs": "^6.6.3"
  },
  "scripts": {
    "build": "tsc",
    "check": "run-s check:prettier check:eslint",
    "check:prettier": "prettier --check src/**/*.{ts,tsx}",
    "check:eslint": "eslint src",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:prettier": "prettier --write src/**/*.{ts,tsx}",
    "fix:eslint": "eslint src --fix"
  },
  "devDependencies": {
    "@types/react": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
